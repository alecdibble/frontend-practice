{{#extend "scss"}}
{{#content "spritesheet"}}
$sprite-settings: (height: {{spritesheet.px.height}}, width: {{spritesheet.px.width}}, imageurl: '{{{spritesheet.escaped_image}}}');
{{/content}}
{{#content "sprites"}}
$sprites: ({{#each sprites}}{{strings.name}}: {{px.offset_x}} {{px.offset_y}}{{#unless @last}}, {{/unless}}{{/each}});
{{/content}}
{{/extend}}
